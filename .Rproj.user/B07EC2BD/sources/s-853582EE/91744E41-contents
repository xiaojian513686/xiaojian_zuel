library(phyloseq)
library(rlang)
library(bitops)
install.packages("rhdf5")
if (!require("BiocManager", quietly = TRUE))
    install.packages("BiocManager")

BiocManager::install("zlibbioc")
library(zlibbioc)
BiocManager::install("rhdf5")
library(rhdf5)
install.packages("devtools")
library(devtools)
devtools::install_github("jacobbien/trac")
library(usethis)
BiocManager::install("usethis")
install.packages("usethis")
options(pkgType="binary")
install.packages("cli")
update.packages("cli")
library(cli)
conda install -c conda-forge r-usethis
library(cli)
sessionInfo()